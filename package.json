{
  "name": "spendro",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack 2>&1 | tee dev.log",
    "build": "bun run next build",
    "start": "bun run next start",
    "check": "bun biome check --write . && tsc --noEmit",
    "ci": "bun run check && bun run build",
    "reset": "bun run --tsconfig-override tsconfig.json scripts/reset-db.ts",
    "db:push": "source .env.local && supabase db push -p $SUPABASE_DB_PW",
    "gen-pdf": "bun run scripts/generate-test-pdf.ts",
    "clean-temp": "rm -rf temp && mkdir -p temp",
    "log:clear": "rm -f dev.log",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "bun biome check --write ."
    ],
    "*.{ts,tsx}": [
      "tscw --noEmit"
    ]
  },
  "dependencies": {
